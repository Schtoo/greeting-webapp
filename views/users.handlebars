<h2>Greeted Users</h2>

<div class='userSide'>
    <form action="/" method="get">
        <div class="row">
            <div class="six columns">
                <button class="homeBtn">home</button>
            </div>
    </form>
    <div class="six columns">
        <form action="/clear" method="post">
            <button class="route" type="submit">clear</button>
        </form>
        <p class="paraColor">Erases Everthing</p>
    </div>
    <div class="row">
        <div class="twelve columns">
            <table>
                <tr>
                    <th>Names greeted</th>
                    <th>Times greeted</th>
                </tr>
                {{#each users}}
                <tr>
                    <td><a href="/counter/{{names}}">{{names}}</a></td>
                    <td>{{count}}</td>
                </tr>
                {{/each}}
            </table>
        </div>
    </div>
</div>